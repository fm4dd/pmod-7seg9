#Pin_in "kypd=row[1]" Loc = "IO_NB_B3";
## #######################################################
## E1 board PMOD-B constraints PMOD 7SEG9 on lower pinrow
## #######################################################
Pin_out "tm_din" Loc = "IO_NB_B6"; # Data
Pin_out "tm_clk" Loc = "IO_NB_B7"; # Clock
